<main class="flex w-full h-screen bg-[#181D31]">
  <%= render 'shared/sidebar' %>
  <div class="w-full">
    <% if notice.present? %>
      <p>aaa</p>
    <% end %>

    <div class="flex justify-between items-center">
      <h1 class="font-bold text-4xl">Rooms</h1>
    </div>
    <%= turbo_frame_tag 'rooms_controller' do %>
        <%= link_to 'New room', new_room_path, class: "w-full text-white bg-primary-600 hover:bg-primary-700 focus:ring-4 focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800" %>
    <% end %>

    <div id="rooms" class="">
      <%= render @rooms %>
    </div>
  </div>
</main>
